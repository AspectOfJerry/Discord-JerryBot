{
    "name": "jerrybot",
    "version": "1.0.0",
    "description": "JerryBot",
    "main": "index.js",
    "scripts": {
        "clearcommands": "node index.js",
        "predeploy": "npm install",
        "deploy": "node index.js",
        "dev": "node index.js",
        "main": "node index.js",
        "test": "node index.js",
        "clean": "node index.js",
        "preclean": "npm install"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AspectOfJerry/JerryBot.git"
    },
    "bugs": {
        "url": "https://github.com/AspectOfJerry/JerryBot/issues",
        "email": "jerry@jerrydev.net"
    },
    "eslintConfig": {
        "env": {
            "es6": true,
            "browser": true,
            "commonjs": false
        },
        "extends": "eslint:recommended",
        "overrides": [],
        "parserOptions": {
            "ecmaVersion": "latest",
            "sourceType": "module"
        },
        "rules": {
            "indent": [
                "warn",
                4
            ],
            "quotes": [
                "error",
                "double"
            ],
            "semi": [
                "error",
                "always"
            ],
            "max-nested-callbacks": [
                "error",
                {
                    "max": 3
                }
            ],
            "no-unused-vars": "warn",
            "no-unreachable": "warn",
            "prefer-const": "warn"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "homepage": "https://bot.jerrydev.net",
    "type": "module",
    "dependencies": {
        "@discordjs/builders": "^0.12.0",
        "@discordjs/rest": "^1.3.0",
        "@discordjs/voice": "^0.13.0",
        "cron": "^1.8.2",
        "dayjs": "^1.11.9",
        "discord-api-types": "^0.37.48",
        "discord-tictactoe": "^3.0.0",
        "discord.js": "^13.16.0",
        "dotenv": "^16.0.0",
        "express": "^4.19.2",
        "mongoose": "^8.0.3",
        "ms": "^2.1.3",
        "node-fetch": "^2.6.7",
        "openai": "^4.17.4",
        "weather-js": "^2.0.0",
        "winston": "^3.9.0",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "eslint": "^8.44.0"
    }
}
